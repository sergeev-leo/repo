{
  "plugins": [
    "react"
  ],
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "warn",
    "eqeqeq": ["error", "always"],
    "quotes": ["error", "single"],
    "jsx-quotes": ["error", "prefer-double"],
    "comma-spacing": ["error", { "before": false, "after": true }],
    "indent": ["error", 2],

    "react/prop-types": "warn"
  }
}